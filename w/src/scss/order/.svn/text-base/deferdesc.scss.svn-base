@import '../common/base.scss';
@import '../common/button.scss';
.banner {
    min-height: 100px;
    padding-bottom: 15px;
    background-color: #eee;
    img {
        display: block;
        max-width: 100%;
        height: auto;
        width: 100%;
    }
}

.maindata {
    padding: 15px 15px;
    >div {
        background-color: #fff;
    }
    .table {
        margin-top: 5px;
        margin-bottom: 15px;
        border: 1px solid #eee;
        >div {
            padding: 10px;
            border-bottom: 1px solid #eee;
        }
    }
    .tableheader {
        background-color: #e5e5e5;
    }
    .flex-1 {
        text-align: center;
    }
    .prospecial,.condition{
        margin-top: 5px;
        margin-bottom: 15px;
        font-size: 12px;
        line-height: 20px;
    }
}

.submitdiv {
    margin: 20px 15px 0 15px;
    padding-bottom: 15px;
    button {
          @include submitbtn;
    }
}