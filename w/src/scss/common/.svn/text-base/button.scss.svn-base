@mixin submitbtn($bgcolor:#2096e9) {
    background-color:$bgcolor;
    font-size: 18px;
    padding: 13px;
    width: 100%;
    color: #fff;
    border: none;
    outline: none;
    border-radius: 4px;
    &:disabled {
        background-color: #dcdcdc;
        color: #000;
    }
}

